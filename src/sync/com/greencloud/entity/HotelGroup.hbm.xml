<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greencloud.entity.HotelGroup" table="hotel_group">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>    
        <property name="code" type="string">
            <column name="code" length="20" />
        </property>
        <property name="sta" type="string">
            <column name="sta" length="2">
                
            </column>
        </property>
        <property name="audit" type="string">
            <column name="audit" length="2" />
        </property>
        <property name="descript" type="string">
            <column name="descript" length="60" />
        </property>
        <property name="descriptEn" type="string">
            <column name="descript_en" length="60" />
        </property>
        <property name="descriptShort" type="string">
            <column name="descript_short" length="20" />
        </property>
        <property name="country" type="string">
            <column name="country" length="3" />
        </property>
        <property name="city" type="string">
            <column name="city" length="60" />
        </property>
        <property name="address1" type="string">
            <column name="address1" />
        </property>
        <property name="address2" type="string">
            <column name="address2" />
        </property>
        <property name="phone" type="string">
            <column name="phone" length="30" />
        </property>
        <property name="fax" type="string">
            <column name="fax" length="30" />
        </property>
        <property name="phoneRsv" type="string">
            <column name="phone_rsv" length="30" />
        </property>
        <property name="website" type="string">
            <column name="website" length="128" />
        </property>
        <property name="email" type="string">
            <column name="email" length="64" />
        </property>
        <property name="dns" type="string">
            <column name="dns" length="64" />
        </property>
        <property name="remark" type="string">
            <column name="remark" length="65535" />
        </property>
        <property name="logo" type="binary">
            <column name="logo" />
        </property>
        <property name="photo" type="string">
            <column name="photo" />
        </property>
        <property name="htmlInfo" type="string">
            <column name="html_info" />
        </property>
        <property name="listOrder" type="java.lang.Integer">
            <column name="list_order" />
        </property>
        <property name="createUser" type="string">
            <column name="create_user" length="20" />
        </property>
        <property name="createDatetime" type="timestamp">
            <column name="create_datetime" length="0" />
        </property>
        <property name="modifyUser" type="string">
            <column name="modify_user" length="20" />
        </property>
        <property name="modifyDatetime" type="timestamp">
            <column name="modify_datetime" length="0" />
        </property>
        <property name="isSingle" type="string">
            <column name="is_single" length="2" />
        </property>
        <property name="appType" type="string">
            <column name="app_type" length="20" />
        </property>
    </class>
</hibernate-mapping>
