<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greencloud.entity.PosConddtl" table="pos_conddtl">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="hotelGroupId" type="java.lang.Long">
            <column name="hotel_group_id" />
        </property>
        <property name="hotelId" type="java.lang.Long">
            <column name="hotel_id" />
        </property>
        <property name="code"  type="string">
            <column name="code" length="30" />
        </property>
        <property name="descript"  type="string">
            <column name="descript" length="60" />
        </property>
        <property name="descriptEn"  type="string">
            <column name="descript_en" length="30" />
        </property>
        <property name="helpcode"  type="string">
            <column name="helpcode" length="20" />
        </property>
        <property name="unit"  type="string">
            <column name="unit" length="4" />
        </property>
         <property name="price" type="big_decimal">
            <column name="price" precision="12"/>
        </property>
         <property name="empflag"  type="string">
            <column name="empflag" length="1" />
        </property>
        <property name="barflag"  type="string">
            <column name="barflag" length="1" />
        </property>
        <property name="condstCode"  type="string">
            <column name="condst_code" length="100" />
        </property>
        <property name="empid"  type="string">
            <column name="empid" length="100" />
        </property>
         <property name="date" type="timestamp">
            <column name="date" length="0" />
        </property>
        <property name="logid" type="integer">
            <column name="logid" length="11"/>
        </property>
        <property name="wpcode"  type="string">
            <column name="wpcode" length="12" />
        </property>
        <property name="numbmode" type="integer">
            <column name="numbmode" length="11"/>
        </property>
        <property name="number" type="big_decimal">
            <column name="number" precision="12"/>
        </property>
        <property name="ratemode" type="integer">
            <column name="ratemode" length="11"/>
        </property>
        <property name="dscmode" type="integer">
            <column name="dscmode" length="11"/>
        </property>
        <property name="dscrate" type="big_decimal">
            <column name="dscrate" precision="12"/>
        </property>
        <property name="paycode"  type="string">
            <column name="paycode" length="5" />
        </property>
         <property name="billprint"  type="string">
            <column name="billprint" length="5" />
        </property>
         <property name="kitchenmsg"  type="string">
            <column name="kitchenmsg" length="1" />
        </property>
         <property name="codeType"  type="string">
            <column name="code_type" length="30" />
        </property>
        <property name="groupCode"  type="string">
            <column name="group_code" length="30" />
        </property> 
        <property name="isGroup"  type="string">
        	<column name="is_group" length="2"></column>
        </property>
        <property name="isHalt"  type="string">
            <column name="is_halt" length="2">
            </column>
        </property>
        <property name="listOrder" type="java.lang.Integer">
            <column name="list_order">
            </column>
        </property>
        <property name="createUser"  type="string">
            <column name="create_user" length="20" />
        </property>
        <property name="createDatetime" type="timestamp">
            <column name="create_datetime" length="0" />
        </property>
        <property name="modifyUser"  type="string">
            <column name="modify_user" length="20" />
        </property>
        <property name="modifyDatetime" type="timestamp">
            <column name="modify_datetime" length="0" />
        </property>
    </class>
</hibernate-mapping>
