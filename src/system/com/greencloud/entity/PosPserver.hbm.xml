<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greencloud.entity.PosPserver" table="pos_pserver">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="hotelGroupId" type="java.lang.Long">
            <column name="hotel_group_id" />
        </property>
        <property name="hotelId" type="java.lang.Long">
            <column name="hotel_id" />
        </property>
        <property name="code"  type="string">
            <column name="code"/>
        </property>
         <property name="descript"  type="string">
            <column name="descript" />
        </property>
         <property name="descriptEn" type="string">
            <column name="descript_en" />
        </property>
        <property name="station" type="string">
            <column name="station" />
        </property>
        <property name="staFreshen" type="java.lang.Integer">
            <column name="sta_freshen" />
        </property>
         <property name="printFreshen"  type="java.lang.Integer">
            <column name="print_freshen"/>
        </property>
        <property name="createUser"  type="string">
            <column name="create_user" length="60" />
        </property>
        <property name="createDatetime" type="timestamp">
            <column name="create_datetime" length="0" />
        </property>
        <property name="modifyUser"  type="string">
            <column name="modify_user" length="60" />
        </property>
        <property name="modifyDatetime" type="timestamp">
            <column name="modify_datetime" length="0" />
        </property>
    </class>
</hibernate-mapping>
