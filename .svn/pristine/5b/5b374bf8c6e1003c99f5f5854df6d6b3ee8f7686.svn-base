<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greencloud.entity.UserPosAuthUserAuth" table="user_pos_auth_user_auth">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="hotelGroupId" type="java.lang.Long">
            <column name="hotel_group_id" />
        </property>
        <property name="hotelId" type="java.lang.Long">
            <column name="hotel_id" />
        </property>
        <property name="userCode"  type="string">
            <column name="user_code" />
        </property>
         <property name="roleCode"  type="string">
            <column name="role_code"/>
        </property>
         <property name="authHotelGroupId"  type="java.lang.Long">
            <column name="auth_hotel_group_id" />
        </property>
         <property name="authHotelId"  type="java.lang.Long">
            <column name="auth_hotel_id" />
        </property>
        <property name="authCode"  type="string">
            <column name="auth_code" />
        </property> 
        <property name="createUser"  type="string">
            <column name="create_user" length="20" />
        </property>
        <property name="createDatetime" type="timestamp">
            <column name="create_datetime" length="0" />
        </property>
        <property name="modifyUser"  type="string">
            <column name="modify_user" length="20" />
        </property>
        <property name="modifyDatetime" type="timestamp">
            <column name="modify_datetime" length="0" />
        </property>
    </class>
</hibernate-mapping>
