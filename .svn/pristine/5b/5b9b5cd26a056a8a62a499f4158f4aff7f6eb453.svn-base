<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greencloud.entity.PosPccode" table="pos_pccode" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="hotelGroupId" type="long">
            <column name="hotel_group_id"  />
        </property>
        <property name="hotelId" type="long">
            <column name="hotel_id"  />
        </property>
         <property name="taCode"  type="string">
            <column name="ta_code" length="10"  >
            </column>
        </property>
        <property name="code"  type="string">
            <column name="code" length="3"  >
            </column>
        </property>
        <property name="descript"  type="string">
            <column name="descript" length="60"  >
            </column>
        </property>
        <property name="descriptEn"  type="string">
            <column name="descript_en" length="30"  >
            </column>
        </property>
        <property name="menuType"  type="string">
            <column name="menu_type" length="10">
            </column>
        </property>
        <property name="dscRate" type="big_decimal">
            <column name="dsc_rate" precision="12">
            </column>
        </property>
        <property name="serveRate" type="big_decimal">
            <column name="serve_rate" precision="12">
            </column>
        </property>
        <property name="taxRate" type="big_decimal">
            <column name="tax_rate" precision="12">
            </column>
        </property>
        <property name="decLength" type="java.lang.Integer">
            <column name="dec_length">
            </column>
        </property>
        <property name="decMode"  type="string">
            <column name="dec_mode" length="1">
            </column>
        </property>
        <property name="groundBmp"  type="string">
            <column name="ground_bmp" length="50">
            </column>
        </property>
        <property name="quantity" type="java.lang.Integer">
            <column name="quantity">
            </column>
        </property>
        <property name="overquan" type="java.lang.Integer">
            <column name="overquan">
            </column>
        </property>
        <property name="deptno"  type="string">
            <column name="deptno" length="5">
            </column>
        </property>
        <property name="language"  type="string">
            <column name="language" length="10">
            </column>
        </property>
        <property name="remark"  type="string">
            <column name="remark">
            </column>
        </property>
        <property name="exp1"  type="string">
            <column name="exp1" length="20" />
        </property>
        <property name="exp2"  type="string">
            <column name="exp2" length="20" />
        </property>
        <property name="exp3"  type="string">
            <column name="exp3" length="20" />
        </property>
        <property name="exp4"  type="string">
            <column name="exp4" length="20" />
        </property>
        <property name="isGroup"  type="string">
            <column name="is_group" length="2" >
            </column>
        </property>
        <property name="groupCode"  type="string">
            <column name="group_code" length="30"  />
        </property>
        <property name="codeType"  type="string">
            <column name="code_type" length="30" />
        </property>
        <property name="isHalt"  type="string">
            <column name="is_halt" length="2">
            </column>
        </property>
        <property name="listOrder" type="int">
            <column name="list_order"  >
            </column>
        </property>
         <property name="createUser"  type="string">
            <column name="create_user" length="20" />
        </property>
        <property name="createDatetime" type="timestamp">
            <column name="create_datetime" length="0" />
        </property>
        <property name="modifyUser"  type="string">
            <column name="modify_user" length="20" />
        </property>
        <property name="modifyDatetime" type="timestamp">
            <column name="modify_datetime" length="0" />
        </property>
    </class>
</hibernate-mapping>
