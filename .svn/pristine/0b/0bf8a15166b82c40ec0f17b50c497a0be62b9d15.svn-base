<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greencloud.entity.PosDrinksConsume" table="pos_drinks_consume_history">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="hotelGroupId" type="java.lang.Long">
            <column name="hotel_group_id" />
        </property>
        <property name="hotelId" type="java.lang.Long">
            <column name="hotel_id" />
        </property>
        
        <property name="accnt" type="java.lang.Long">
            <column name="accnt" />
        </property>
       
        <property name="descript"  type="string">
            <column name="descript" length="20" />
        </property>
        
        <property name="volume"  type="string">
            <column name="volume" length="20" />
        </property>
            
         <property name="consumeNumber"  type="java.lang.Integer">
            <column name="consume_amount"  />
        </property>
   
        
        <property name="createUser"  type="string">
            <column name="create_user" length="20" />
        </property>
        <property name="createDatetime" type="timestamp">
            <column name="create_datetime" length="0" />
        </property>
        <property name="modifyUser"  type="string">
            <column name="modify_user" length="20" />
        </property>
        <property name="modifyDatetime" type="timestamp">
            <column name="modify_datetime" length="0" />
        </property>
    </class>
</hibernate-mapping>
