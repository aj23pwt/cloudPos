-- 注意： 如果是餐饮本地库 需要新建日志表
 -- DROP TABLE IF EXISTS `log_info_code`;
CREATE TABLE `log_info_code` (
  `hotel_group_id` bigint(16) DEFAULT NULL,
  `hotel_id` bigint(16) DEFAULT NULL,
  `id` bigint(16) NOT NULL AUTO_INCREMENT,
  `code` varchar(20) DEFAULT NULL,
  `sql_define` varchar(2000) DEFAULT NULL,
  `create_user` varchar(20) DEFAULT NULL COMMENT '创建人',
  `create_datetime` datetime DEFAULT NULL COMMENT '创建时间',
  `modify_user` varchar(20) DEFAULT NULL COMMENT '修改人',
  `modify_datetime` datetime DEFAULT NULL COMMENT '修改时间',
  PRIMARY KEY (`id`),
  KEY `NewIndex1` (`hotel_group_id`,`hotel_id`,`code`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
-- DROP TABLE IF EXISTS `log_info`;
CREATE TABLE `log_info` (
  `hotel_group_id` bigint(16) DEFAULT NULL COMMENT '酒店集团id',
  `hotel_id` bigint(16) DEFAULT NULL COMMENT '酒店id',
  `id` bigint(16) NOT NULL AUTO_INCREMENT,
  `entity_name` varchar(30) DEFAULT NULL,
  `entity_id` bigint(16) NOT NULL COMMENT '实体id',
  `column_name` varchar(256) NOT NULL COMMENT '列名',
  `descript` varchar(60) NOT NULL COMMENT '列描述',
  `descript_en` varchar(60) DEFAULT NULL,
  `old_value` varchar(1000) DEFAULT NULL COMMENT '旧值',
  `new_value` varchar(1000) DEFAULT NULL COMMENT '新值',
  `station_code` varchar(20) NOT NULL COMMENT '客户工作站code',
  `need_translate` varchar(2) DEFAULT NULL COMMENT '是否需要代码翻译',
  `create_user` varchar(60) DEFAULT NULL COMMENT '创建人姓名',
  `create_datetime` datetime NOT NULL COMMENT '创建时间',
  PRIMARY KEY (`id`),
  KEY `NewIndex1` (`hotel_group_id`,`hotel_id`,`entity_name`,`entity_id`),
  KEY `NewIndex2` (`hotel_group_id`,`hotel_id`,`entity_name`,`create_user`,`create_datetime`),
  KEY `NewIndex3` (`hotel_group_id`,`entity_name`,`entity_id`),
  KEY `NewIndex4` (`hotel_group_id`,`entity_name`,`entity_id`,`create_datetime`),
  KEY `modeindex_entity_name_id` (`entity_name`,`entity_id`),
  KEY `Index_create_datetime` (`hotel_group_id`,`hotel_id`,`create_datetime`)
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COMMENT='针对各种实体entry的增删改日志，比如宾客、订单等';
-- DROP TABLE IF EXISTS `log_info_config`;
CREATE TABLE `log_info_config` (
  `hotel_group_id` bigint(16) DEFAULT NULL,
  `hotel_id` bigint(16) DEFAULT NULL,
  `id` bigint(16) NOT NULL AUTO_INCREMENT,
  `entity_name` varchar(100) DEFAULT NULL COMMENT '标识核心业务code,如果只有一个业务实体,则和字段业务实体一致,如果客户服务guest',
  `entity_column` varchar(50) DEFAULT NULL COMMENT '标识业务实体中需要监控的字段，多选";"分隔.如果为null,则表示该实体不记录任何字段信息',
  `descript` varchar(60) DEFAULT NULL,
  `descript_en` varchar(60) DEFAULT NULL,
  `sql_define` varchar(2000) DEFAULT NULL COMMENT 'SQL定义用于对值的代码翻译',
  `create_user` varchar(20) DEFAULT NULL COMMENT '创建人',
  `create_datetime` datetime DEFAULT NULL COMMENT '创建时间',
  `modify_user` varchar(20) DEFAULT NULL COMMENT '修改人',
  `modify_datetime` datetime DEFAULT NULL COMMENT '修改时间',
  PRIMARY KEY (`id`),
  KEY `NewIndex1` (`hotel_group_id`,`hotel_id`,`entity_name`)
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8;
