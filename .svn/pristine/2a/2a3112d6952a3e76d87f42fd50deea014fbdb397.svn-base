        <property name="descript" type="string">
            <column name="descript" length="30" />
        </property>
        <property name="descriptEn" type="string">
            <column name="descript_en" length="80" />
        </property>
        <property name="noteCode" type="string">
            <column name="note_code" length="10" />
        </property>
        <property name="sortCode" type="string">
            <column name="sort_code" length="10" />
        </property>
        <property name="pluCode" type="string">
            <column name="plu_code" length="10" />
        </property>
        <property name="pinumber" type="java.lang.Integer">
            <column name="pinumber"/>
        </property>
        <property name="unit" type="string">
            <column name="unit" length="4" />
        </property>
        <property name="price" type="big_decimal">
            <column name="price" precision="12"/>
        </property>
        <property name="number" type="big_decimal">
            <column name="number" precision="12"/>
        </property>
        <property name="amount" type="big_decimal">
            <column name="amount" precision="12"/>
        </property>
        <property name="sta" type="string">
            <column name="sta" length="1" />
        </property>
         <property name="flag" type="string">
            <column name="flag" length="30" />
        </property>
         <property name="flag1" type="string">
            <column name="flag1" length="30" />
        </property>
         <property name="empid" type="string">
            <column name="empid" length="10" />
        </property>
        <property name="logdate" type="timestamp">
            <column name="logdate" length="0" />
        </property>
         <property name="saleid" type="string">
            <column name="saleid" length="10" />
        </property>
         <property name="tableno" type="string">
            <column name="tableno" length="6" />
        </property>
         <property name="siteno" type="string">
            <column name="siteno" length="5" />
        </property>
         <property name="cook" type="string">
            <column name="cook" length="200" />
        </property>
         <property name="remark" type="string">
            <column name="remark" length="30" />
        </property>
         <property name="printer" type="string">
            <column name="printer" length="20" />
        </property>
         <property name="empid1" type="string">
            <column name="empid1" length="10" />
        </property>
         <property name="empid2" type="string">
            <column name="empid2" length="10" />
