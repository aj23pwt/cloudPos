<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greencloud.entity.PosResOrder" table="pos_res_order">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="hotelGroupId" type="java.lang.Long">
            <column name="hotel_group_id" />
        </property>
        <property name="hotelId" type="java.lang.Long">
            <column name="hotel_id" />
        </property>
        <property name="pcid" type="string">
            <column name="pcid" length="20" />
        </property>
         <property name="accnt" type="string">
            <column name="accnt" length="13" />
        </property>
        <property name="inumber" type="java.lang.Integer">
            <column name="inumber"/>
        </property>
        <property name="tnumber" type="java.lang.Integer">
            <column name="tnumber"/>
        </property>
         <property name="anumber" type="java.lang.Integer">
            <column name="anumber"/>
        </property>
        <property name="mnumber" type="java.lang.Integer">
            <column name="mnumber"/>
        </property>
         <property name="type" type="string">
            <column name="type" length="1" />
