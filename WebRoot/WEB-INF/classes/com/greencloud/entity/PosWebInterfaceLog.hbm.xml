<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greencloud.entity.PosWebInterfaceLog" table="pos_web_interface_log" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="hotelGroupId" type="long">
            <column name="hotel_group_id" not-null="true" />
        </property>
        <property name="hotelId" type="long">
            <column name="hotel_id" not-null="true" />
        </property>
        <property name="hotelGroupCode" type="string">
            <column name="hotel_group_code" />
        </property>
        <property name="hotelCode" type="string">
            <column name="hotel_code" />
        </property>
        <property name="logType" type="string">
            <column name="log_type" />
        </property>
        <property name="model" type="string">
            <column name="model" />
        </property>
        <property name="messageType" type="string">
            <column name="message_type" />
        </property>
        <property name="messageNo" type="string">
            <column name="message_no" />
        </property>
        <property name="status" type="string">
            <column name="status" />
        </property>
        <property name="posRsvNo" type="string">
            <column name="pos_rsv_no" />
        </property>
        <property name="otherNo" type="string">
            <column name="other_no" />
        </property>
        <property name="resource" type="string">
            <column name="resource" />
        </property>
        <property name="openid" type="string">
            <column name="openid" />
        </property>
         <property name="appid" type="string">
            <column name="appid" />
        </property>
        <property name="requestXml" type="string">
            <column name="request_xml" />
        </property>
        <property name="resultXml" type="string">
            <column name="result_xml" />
        </property>
        <property name="remark" type="string">
            <column name="remark" />
        </property>
        <property name="bizDate" type="timestamp">
            <column name="biz_date" length="0" />
        </property>
        <property name="flag" type="string">
            <column name="flag" />
        </property>
        <property name="createDatetime" type="timestamp">
            <column name="create_datetime" length="0" not-null="true" />
        </property>
        <property name="createUser" type="string">
            <column name="create_user" length="20" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
