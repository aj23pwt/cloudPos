<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greencloud.entity.PosAuditReport" table="pos_audit_report">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="hotelGroupId" type="java.lang.Long">
            <column name="hotel_group_id" />
        </property>
        <property name="hotelId" type="java.lang.Long">
            <column name="hotel_id" />
        </property>
        <property name="bizDate" type="timestamp">
            <column name="biz_date" length="0" />
        </property>
        <property name="type"  type="string">
            <column name="type" length="1" />
        </property>
         <property name="accnt"  type="string">
            <column name="accnt" length="14" />
        </property>
         <property name="userCode"  type="string">
            <column name="userCode" length="10" />
        </property>
   		<property name="shift"  type="string">
            <column name="shift" length="1" />
        </property>
         <property name="pccode"  type="string">
            <column name="pccode" length="10" />
        </property>
         <property name="name"  type="string">
            <column name="name" length="20" />
        </property>
         <property name="code"  type="string">
            <column name="code" length="10" />
        </property>
         <property name="descript"  type="string">
            <column name="descript" length="20" />
        </property>
         
        <property name="amount" type="big_decimal">
            <column name="amount" precision="12"/>
        </property>
        <property name="amount0" type="big_decimal">
            <column name="amount0" precision="12"/>
        </property>
        <property name="amount1" type="big_decimal">
            <column name="amount1" precision="12"/>
        </property>
        <property name="amount2" type="big_decimal">
            <column name="amount2" precision="12"/>
        </property>
        <property name="amount3" type="big_decimal">
            <column name="amount3" precision="12"/>
        </property>
        
        <property name="createUser"  type="string">
            <column name="create_user" length="20" />
        </property>
        <property name="createDatetime" type="timestamp">
            <column name="create_datetime" length="0" />
        </property>
        <property name="modifyUser"  type="string">
            <column name="modify_user" length="20" />
        </property>
        <property name="modifyDatetime" type="timestamp">
            <column name="modify_datetime" length="0" />
        </property>
    </class>
</hibernate-mapping>
